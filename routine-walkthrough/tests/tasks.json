{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "convert file",
            "type": "shell",
            "command": "${workspaceFolder}/venv/Scripts/pyuic6.exe",
            "args": [
                "${file}", "-o", "${workspaceFolder}/ui_${fileBasenameNoExtension}.py"
            ]
        },
        {
            "label": "launch",
            "type": "shell",
            "command": "${workspaceFolder}/venv/Scripts/python.exe main.py"
        }
    ]
}